{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "var": {
    "via": "<default>via</>"
  },
  "transient_prompt": {
    "template": "> ",
    "foreground": "green",
    "background": "transparent"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "root | ",
          "foreground": "red",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "style": "full"
          },
          "template": "<b>{{ .Path }}</b> ",
          "foreground": "cyan",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_status": true
          },
          "template": "<default>on</> <b><magenta>{{ .HEAD }}</> <yellow>{{if .BranchStatus }}{{ .BranchStatus }}{{ end }}{{ if or (.Working.Changed) (.Staging.Changed) }} [{{ end }}{{ if .Staging.Changed }}\uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} | {{ end }}{{ if .Working.Changed }}\uf044 {{ .Working.String }}{{ end }}{{ if or (.Working.Changed) (.Staging.Changed) }}]{{ end }}</b></> ",
          "foreground": "#F1502F",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Var.via }} <b>\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}v{{ .Full }}</b>",
          "foreground": "#6CA35E",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "context",
            "fetch_virtual_env": true
          },
          "template": "{{ .Var.via}} <b>\ue235 {{ if .Venv }}{{ .Venv }} {{ end }}v{{ .Full }}</b>",
          "foreground": "#4584b6",
          "type": "python",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ .Var.via}} <b>\ue620 v{{ .Full }}</b>",
          "foreground": "blue",
          "type": "lua",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<default>took</> <b>{{ .FormattedMs }}</b> |",
          "foreground": "yellow",
          "type": "executiontime"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "foreground": "lightGreen",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "âžœ ",
          "foreground": "lightGreen",
          "type": "status",
          "style": "powerline",
          "foreground_templates": [
            "{{ if gt .Code 0 }}red{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
